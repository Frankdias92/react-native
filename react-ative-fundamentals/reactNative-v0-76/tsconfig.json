{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/src*": ["./src/*"],
      "@/app*": ["./src/app/*"],
      "@/components*": ["./src/app/components/*"],
      "@/context*": ["./src/app/context/*"],
      "@/hooks*": ["./src/app/hooks/*"],
      "@/dtos*": ["./src/app/dtos/*"],
      "@/storage*": ["./src/app/storage/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}