{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@src*": ["./src/"],
      "@app*": ["./src/app"],
      "@assets*": ["./src/assets"],
      "@components*": ["./src/components"],
      "@/*": ["./src/*"],

    },
    "types": ["nativewind/types"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
